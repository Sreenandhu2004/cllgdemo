#include<stdio.h>
int main(){
    int a[10],i;
    printf("enter 10 numbers:");
    for(i=0;i<10;i++){
        scanf("%d",&a[i]);
    }
    int even=0,odd=0,zero=0;
    for(i=0;i<10;i++){
        if(a[i]%2==0 && a[i]!=0){
            even++;
        }
        else if(a[i]%2!=0){
            odd++;
        }
        else{
            zero++;
        }
    }
    printf("\nno.of even numbers:%d",even);
    printf("\nno.of odd no:%d",odd);
    printf("\nno.of zeros:%d",zero);
}