#include<stdio.h>
int main(){
    int a[10],i;
    printf("enter 10 numbers:");
    for(i=0;i<10;i++){
        scanf("%d",&a[i]);
    }
    int pos=0,neg=0,zero=0;
    for(i=0;i<10;i++){
        if(a[i]>0){
            pos++;
        }
        else if(a[i]<0){
            neg++;
        }
        else{
            zero++;
        }
    }
    printf("\nno.of positive numbers:%d",pos);
    printf("\nno.of negative no:%d",neg);
    printf("\nno.of zeros:%d",zero);
}